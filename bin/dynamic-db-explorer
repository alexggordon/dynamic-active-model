#!/usr/bin/env ruby
# frozen_string_literal: true

require 'dynamic-active-model'

options = {
  connection_options: {
    username: 'root',
    adapter: 'mysql2',
    database: 'test'
  },
  skip_tables: [],
  relationsips: {}
}

# rubocop:disable Style/Documentation
module DB
end
# rubocop:enable Style/Documentation

DynamicActiveModel::Explorer.explore(
  DB,
  options[:connection_options],
  options[:skip_tables],
  options[:relationsips]
)

require 'irb'
IRB.start(__FILE__)
